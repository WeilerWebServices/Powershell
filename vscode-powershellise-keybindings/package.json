{
    "name": "powershellise-keybindings",
    "displayName": "PowerShell ISE Keymap",
    "description": "Popular PowerShell ISE keybindings for Visual Studio Code",
    "version": "0.0.1",
    "publisher": "ms-vscode",
    "preview": true,
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Keymaps"
    ],
    "keywords": [
        "keymap", "PowerShell", "PowerShell ISE"
    ],
    "galleryBanner": {
        "color": "#ACD1EC",
        "theme": "light"
    },
    "license": "SEE LICENSE IN LICENSE",
    "icon": "images/PowerShellISE_icon.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/PowerShell/vscode-powershellise-keybindings.git"
    },    
    "bugs": {
        "url": "https://github.com/PowerShell/vscode-powershellise-keybindings/issues"
    },    
    "contributes": {
        "keybindings": [
            {
                "mac": "cmd+i",
                "key": "ctrl+i",
                "command": "workbench.action.focusActiveEditorGroup"
            },
            {
                "mac": "cmd+d",
                "key": "ctrl+d",
                "command": "workbench.action.terminal.focus"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "@types/node": "^6.0.40"
    }
}